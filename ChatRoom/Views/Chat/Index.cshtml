@{
}

<div class="row">
    <div class="col-md-8 offset-2">
        <h2>Salas de chat</h2>
        <ul class="list-group">
            @foreach ( var e in ChatRoom.Controllers.ChatController.Rooms)
            {
                <li class="list-group-item">
                    <a href="~/chat/Room?room=@e.Key">@e.Value</a>
                </li>
            }

        </ul>

    </div>

</div>